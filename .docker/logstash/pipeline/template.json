{
  "index_patterns": ["logs-*"],
  "mappings": {
    "properties": {
      "@timestamp": {
        "type": "date"
      },
      "level": {
        "type": "keyword"
      },
      "logger": {
        "type": "keyword"
      },
      "thread": {
        "type": "keyword"
      },
      "message": {
        "type": "text",
        "index": false
      },
      "counter": {
        "type": "long"
      }
    }
  }
}